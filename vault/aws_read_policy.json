{
   "Version": "2012-10-17",
   "Statement": [
     {
       "Effect": "Allow",
       "Action": ["s3:ListBucket"],
       "Resource": ["arn:aws:s3:::tfremotestatenic"]
     },
     {
       "Effect": "Allow",
       "Action": [
         "s3:GetObject"
       ],
       "Resource": ["arn:aws:s3:::tfremotestatenic/*"]
     },
     {
         "Effect": "Allow",
         "Action": "ec2:Describe*",
         "Resource": "*"
     },
     {
         "Effect": "Allow",
         "Action": "elasticloadbalancing:Describe*",
         "Resource": "*"
     },
     {
         "Effect": "Allow",
         "Action": [
             "cloudwatch:ListMetrics",
             "cloudwatch:GetMetricStatistics",
             "cloudwatch:Describe*"
         ],
         "Resource": "*"
     },
     {
         "Effect": "Allow",
         "Action": "autoscaling:Describe*",
         "Resource": "*"
     }

   ]
 }
